{
  "name": "thorswap-web-app-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "GENERATE_SOURCEMAP=false craco --max_old_space_size=4096 build",
    "build:dev": "yarn build && yarn release-storybook",
    "test": "craco test",
    "eject": "craco eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "release-storybook": "build-storybook -o ./build/storybook",
    "prepare": "husky install",
    "lint": "NODE_OPTIONS=--max_old_space_size=4096 eslint src --ext .ts,.tsx",
    "lint:fix": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,css}' && yarn lint --fix",
    "typescript": "tsc --noEmit"
  },
  "dependencies": {
    "@fontsource/poppins": "^4.5.0",
    "@headlessui/react": "^1.4.3",
    "@reduxjs/toolkit": "^1.7.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@thorswap-lib/midgard-sdk": "^0.0.2",
    "@thorswap-lib/multichain-sdk": "^0.0.1",
    "axios": "^0.25.0",
    "babel-loader": "8.1.0",
    "chart.js": "^3.7.0",
    "classnames": "^2.3.1",
    "cssnano": "^5.0.15",
    "eslint": "^7.11.0",
    "eslint-plugin-storybook": "^0.5.5",
    "i18next": "21.6.11",
    "react": "^17.0.2",
    "react-chartjs-2": "^4.0.1",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^17.0.2",
    "react-hot-toast": "2.2.0",
    "react-i18next": "11.15.4",
    "react-icons": "^4.3.1",
    "react-redux": "^7.2.6",
    "react-router": "^6.2.1",
    "react-router-dom": "6.2.1",
    "react-table": "^7.7.0",
    "react-tooltip": "4.2.21",
    "rebass": "^4.0.7",
    "redux": "^4.1.2",
    "redux-logger": "^3.0.6",
    "swiper": "^6.8.1",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.17.0",
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@craco/craco": "^6.4.3",
    "@storybook/addon-actions": "6.5.0-alpha.5",
    "@storybook/addon-essentials": "6.5.0-alpha.5",
    "@storybook/addon-links": "6.5.0-alpha.5",
    "@storybook/builder-webpack5": "6.5.0-alpha.5",
    "@storybook/manager-webpack5": "6.5.0-alpha.5",
    "@storybook/node-logger": "6.5.0-alpha.5",
    "@storybook/preset-create-react-app": "4.0.0",
    "@storybook/react": "6.5.0-alpha.5",
    "@types/eslint": "^8.2.2",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^17.0.20",
    "@types/react-custom-scrollbars": "^4.0.10",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.22",
    "@types/react-table": "^7.7.9",
    "@types/rebass": "^4.0.10",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "autoprefixer": "^10.4.2",
    "craco-alias": "3.0.1",
    "env-cmd": "^10.1.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-watch": "^8.0.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.1.7",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "prettier": "^2.5.1",
    "react-scripts": "5.0.0",
    "storybook-tailwind-dark-mode": "^1.0.11",
    "tailwindcss": "^3.0.15",
    "terser-webpack-plugin": "^5.3.0",
    "typescript": "^4.4.2",
    "webpack": "5.0.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx,json,css}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
